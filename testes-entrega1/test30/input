(ignore-value (setf j1 (cria-jogo t1)))
(ignore-value (setf novo (copia-jogo j1)))
(ignore-value (jogo-aplica-jogada! j1 1))
(ignore-value (jogo-aplica-jogada! j1 2))
(ignore-value (jogo-aplica-jogada! j1 3))
(ignore-value (jogo-aplica-jogada! j1 4))
(ignore-value (jogo-aplica-jogada! novo 5))
(ignore-value (jogo-aplica-jogada! novo 6))
(ignore-value (jogo-aplica-jogada! novo 7))
(desenha-jogo novo)
(desenha-jogo j1)
(jogo-jogador novo)
(jogo-jogador j1)
(jogo-historico-jogadas novo)
(jogo-historico-jogadas j1)
