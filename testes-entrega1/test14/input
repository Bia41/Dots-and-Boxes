(ignore-value (setf temp-t1 (cria-tabuleiro 3 3)))
(ignore-value (tabuleiro-adiciona-moeda-posicao! temp-t1 (cria-posicao 2 1) 6))
(ignore-value (tabuleiro-adiciona-moeda-posicao! temp-t1 (cria-posicao 2 0) 5))
(ignore-value (tabuleiro-adiciona-fio! temp-t1 (cria-posicao 2 1) (cria-posicao 2 0)))
(length (tabuleiro-fios temp-t1))
(every #'fio-p (tabuleiro-fios temp-t1))
(conjuntos-iguais-p (tabuleiro-fios temp-t1) (tabuleiro-fios-posicao temp-t1 (cria-posicao 2 1)) :test #'fios-iguais-p)
(conjuntos-iguais-p (tabuleiro-fios temp-t1) (tabuleiro-fios-posicao temp-t1 (cria-posicao 2 0)) :test #'fios-iguais-p)
(conjuntos-iguais-p (tabuleiro-fios temp-t1) (list (cria-fio 1 (cria-posicao 2 1) (cria-posicao 2 0))) :test #'fios-iguais-p)
(fios-iguais-p (first (tabuleiro-fios temp-t1)) (first (tabuleiro-fios-posicao temp-t1 (cria-posicao 2 1))))
(fios-iguais-p (first (tabuleiro-fios temp-t1)) (first (tabuleiro-fios-posicao temp-t1 (cria-posicao 2 0))))
(fios-iguais-p (first (tabuleiro-fios temp-t1)) (tabuleiro-fio-com-id temp-t1 1))
(desenha-tabuleiro temp-t1)
