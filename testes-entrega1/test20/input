(ignore-value (setf novo (copia-tabuleiro t1)))
(ignore-value (tabuleiro-adiciona-moeda-posicao! t1 (cria-posicao 0 2) 8))
(ignore-value (tabuleiro-adiciona-fio! t1 (cria-posicao 0 2) (cria-posicao 0 1)))
(length (tabuleiro-fios t1))
(length (tabuleiro-fios novo))
(tabuleiro-moeda-posicao t1 (cria-posicao 0 2))
(tabuleiro-moeda-posicao novo (cria-posicao 0 2))
(desenha-tabuleiro t1)
(desenha-tabuleiro novo)
